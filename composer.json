{
    "name": "headlesswpadmin/headless-wp-admin",
    "description": "AdministraciÃ³n headless para WordPress con interfaz moderna",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Benito Anagua",
            "email": "benito.anagua@gmail.com",
            "homepage": "https://benitoanagua.me"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "^3.7",
        "wp-coding-standards/wpcs": "^3.0",
        "phpstan/extension-installer": "^1.3",
        "szepeviktor/phpstan-wordpress": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "HeadlessWPAdmin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HeadlessWPAdmin\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpstan": "phpstan analyse",
        "phpcs": "phpcs --standard=WordPress src/",
        "phpcbf": "phpcbf --standard=WordPress src/",
        "test": [
            "@phpstan",
            "@phpcs"
        ]
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
